{
  "expo": {
    "name": "Send Your Work",
    "slug": "send-your-work-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#111111"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#111111"
      },
      "package": "com.imartiu.sendyourworkmobileapp"
    },
    "userInterfaceStyle": "dark"
  }
}
